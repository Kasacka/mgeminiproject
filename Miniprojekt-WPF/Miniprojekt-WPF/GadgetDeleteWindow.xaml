<Window x:Class="Miniprojekt_WPF.GadgetDeleteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Miniprojekt_WPF"
        mc:Ignorable="d"
        Title="GadgetDeleteWindow" Height="120" Width="300">
    <StackPanel>
        <Label Content="Are you sure that you want to delete this gadget?" />
        <Label />
        <Button Content="Yes" Click="YesClicked" />
        <Button Content="No" Click="NoClicked" />
    </StackPanel>
</Window>
